{
  "name": "hephaestus",
  "version": "0.0.0",
  "scripts": {
    "docker:build": "docker build --rm -f \"Dockerfile\" -t hephaestus:latest \".\"",
    "docker:run": "docker run --rm -d -p 3000:3000/tcp hephaestus:latest",
    "nest:prebuild": "rimraf dist/server",
    "nest:build": "npm run nest:prebuild && nest build",
    "nest:format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "nest:start": "nest start",
    "nest:start:dev": "nest start --watch",
    "nest:start:debug": "nest start --debug --watch",
    "nest:start:prod": "node dist/main",
    "nest:lint": "eslint \"server/{src,apps,libs,test}/**/*.ts\" --fix",
    "ng": "ng",
    "ng:start": "ng serve",
    "ng:prebuild": "rimraf dist/client",
    "ng:build": "npm run ng:prebuild && ng build",
    "ng:lint": "ng lint",
    "test": "jest --config=jest.config.js",
    "test:watch": "jest --watch --config=jest.config.js",
    "test:coverage": "jest --coverage --config=jest.config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.4",
    "@angular/common": "~9.0.4",
    "@angular/compiler": "~9.0.4",
    "@angular/core": "~9.0.4",
    "@angular/forms": "~9.0.4",
    "@angular/platform-browser": "~9.0.4",
    "@angular/platform-browser-dynamic": "~9.0.4",
    "@angular/router": "~9.0.4",
    "@godaddy/terminus": "^4.3.1",
    "@nestjs/common": "^6.11.9",
    "@nestjs/config": "^0.2.2",
    "@nestjs/core": "^6.11.9",
    "@nestjs/platform-fastify": "^6.11.9",
    "@nestjs/swagger": "^4.3.2",
    "@nestjs/terminus": "^6.5.6",
    "fastify-swagger": "^2.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "~6.5.4",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.14.2",
    "@nestjs/schematics": "^6.9.4",
    "@nestjs/testing": "^6.11.9",
    "@types/node": "^13.7.7",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "@angular-devkit/build-angular": "~0.900.4",
    "@angular/cli": "~9.0.4",
    "@angular/compiler-cli": "~9.0.4",
    "@angular/language-service": "~9.0.4",
    "@types/jest": "^25.1.3",
    "codelyzer": "^5.2.1",
    "jest": "^25.1.0",
    "jest-preset-angular": "^8.0.0",
    "protractor": "~5.4.3",
    "tslint": "~6.0.0",
    "typescript": "~3.7.5"
  }
}
